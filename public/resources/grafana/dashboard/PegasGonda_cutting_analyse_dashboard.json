      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "aa8dda4d-edd1-4d46-8e3b-f6c06c741c1e"
          },
          "hide": false,
          "query": "// --------- PowerOn minutes/day (seconds == 59 occurs once per ON-minute) ----------\npowerOnDaily =\n  from(bucket: \"machine_data\")\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n    |> filter(fn: (r) => r._measurement == \"pegas_data\")\n//    |> filter(fn: (r) => r.topic =~ /^Pegas-Gonda\\/CloudConnector\\/ie\\/d\\/j\\/simatic\\/v1\\/s7c1\\/dp\\/r\\/(${device:regex})\\/default$/)\n    |> filter(fn: (r) => r.name == \"Diagnostics_DB.Timers.General.ActualPowerOn.Seconds\")\n    |> filter(fn: (r) => r._field == \"val\")\n    |> filter(fn: (r) => r._value == 59)\n    |> aggregateWindow(every: 1d, offset: 1s, fn: count, createEmpty: false)\n    |> rename(columns: {_value: \"powerOnMin\"})\n    |> keep(columns: [\"_time\", \"topic\", \"powerOnMin\"])\n\n// --------- Production minutes/day (minute has production if Overall_cuts increased) ----------\nprodDaily =\n  from(bucket: \"machine_data\")\n    |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n    |> filter(fn: (r) => r._measurement == \"pegas_data\")\n//    |> filter(fn: (r) => r.topic =~ /^Pegas-Gonda\\/CloudConnector\\/ie\\/d\\/j\\/simatic\\/v1\\/s7c1\\/dp\\/r\\/(${device:regex})\\/default$/)\n    |> filter(fn: (r) => r.name == \"Diagnostics_DB.Cutting.Overall_cuts\")\n    |> filter(fn: (r) => r._field == \"val\")\n    |> aggregateWindow(every: 1m, offset: 1s, fn: last, createEmpty: false)\n    |> difference(nonNegative: true)\n    |> map(fn: (r) => ({ r with _value: if r._value > 0.0 then 1.0 else 0.0 })) // 1 = producing in that minute\n    |> aggregateWindow(every: 1d, offset: 1s, fn: sum, createEmpty: false)\n    |> rename(columns: {_value: \"prodMin\"})\n    |> keep(columns: [\"_time\", \"topic\", \"prodMin\"])\n\n// --------- Join + compute ProductionTime / DownTime / OffTime (hours) ----------\njoin(tables: {p: powerOnDaily, pr: prodDaily}, on: [\"_time\", \"topic\"])\n  |> map(fn: (r) => {\n      powerOnMin = float(v: r.powerOnMin)\n      prodMin = float(v: r.prodMin)\n\n      // clamp to avoid negatives if clocks/data jitter\n      prodMin2 = if prodMin > powerOnMin then powerOnMin else prodMin\n      downMin = if powerOnMin - prodMin2 < 0.0 then 0.0 else (powerOnMin - prodMin2)\n\n      // Off minutes = 1440 - powerOnMin (see DST note below)\n      offMin = if 1440.0 - powerOnMin < 0.0 then 0.0 else (1440.0 - powerOnMin)\n\n      return {\n        date: r._time,\n        ProductionTime: prodMin2 / 60.0,\n        DownTime: downMin / 60.0,\n        OffTime: offMin / 60.0\n      }\n    })\n  |> keep(columns: [\"date\", \"ProductionTime\", \"DownTime\", \"OffTime\", \"topic\"])\n  |> yield(name: \"daily_times\")",
          "refId": "A"
        }
      ],
      "title": "Effectivity = Cuts / Cutting time",
      "transformations": [
        {
          "id": "renameByRegex",
          "options": {
            "regex": ".*_DB.*\\.(.*)\",.*Connector\\/(.*).+",
            "renamePattern": "$2 - $1"
